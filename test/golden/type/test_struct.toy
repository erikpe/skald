extern fn print_i64(x: i64) -> unit;

struct Pair {
  left: i64;
  right: i64;
}

struct Wrapper {
  pair: *Pair;
  ok: bool;
}

struct Node {
  next: *Node;
  value: i64;
}

fn is_null_pair(p: *Pair) -> bool {
  return p == null;
}

fn same_pair(a: *Pair, b: *Pair) -> bool {
  return a == b;
}

fn main() -> i64 {
  var p0: *Pair = null;
  var p1: *Pair = p0;
  var w: *Wrapper = null;
  var n: *Node = null;

  if is_null_pair(p0) {
    print_i64(1);
  } else {
    print_i64(0);
  }

  if same_pair(p0, p1) {
    print_i64(1);
  } else {
    print_i64(0);
  }

  if w == null {
    print_i64(1);
  } else {
    print_i64(0);
  }

  if n != null {
    print_i64(0);
  } else {
    print_i64(1);
  }

  return 0;
}
