extern fn print_i64(x: i64) -> unit;

fn main() -> i64 {
  var x: i64 = 100;
  defer print_i64(x + 1);
  x = 200;
  defer print_i64(x + 2);

  print_i64(333);
  if x == 220 {
    defer print_i64(300);
    return 1;
    print_i64(123);
    defer print_i64(400);
  }
  defer print_i64(500);
  print_i64(444);
  return 2;
}
